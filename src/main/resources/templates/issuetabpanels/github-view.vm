<div class="github-comment issue-data-block activity-github-comment twixi-block expanded">
#if ($entry.isError())
<div class="notify error">$entry.getText()</div>
#elseif ($entry.isComment())
    <div class="twixi-wrap verbose actionContainer">
        <a class="twixi" href="#"><span class="icon twixi-opened"><span>Hide</span></span></a>
        <div class="action-links">
            <a class="icon icon-github" href="$entry.url"><span>Github</span></a>
        </div>
        <div class="action-details">
            <a href="#" class="user-hover user-avatar" style="
                background-image:
                url('$github.getGravatarUrl()'),
                url(data:image/png;base64,)
">
                Some user</a>
            added a code comment
            <abbr class="date" title="$entry.getDate()">$entry.getRelativeDate()</abbr>
            <a href="$entry.url"><abbr class="sha"> $entry.getShortSha()</abbr></a>
            in <a href="$entry.getRepoUrl()">$entry.repo</a>

        </div>
        <div class="action-body flooded">
            $entry.getText()
        </div>
    </div>
    <div class="twixi-wrap concise actionContainer">
        <div class="action-details">
            <a class="twixi" href="#"><span class="icon twixi-opened"><span>Show</span></span></a>
            <a href="#" class="user-hover user-avatar" style="
                background-image:
                url('$github.getGravatarUrl()'),
                url(data:image/png;base64,)
">
                Some user</a>
            added a code comment
            <abbr class="date" title="$entry.getDate()">$entry.getRelativeDate()</abbr>
            <a href="$entry.url"><abbr class="sha"> $entry.getShortSha()</abbr></a>
            in <a href="$entry.getRepoUrl()">$entry.repo</a>
        </div>
    </div>
#else
<div>
    <div class="twixi-wrap verbose actionContainer">
        <a class="twixi" href="#"><span class="icon twixi-opened"><span>Show</span></span></a>
        <div class="action-links">
            <a class="icon icon-github" href="$entry.url"><span>Github</span></a>
        </div>
        <div class="action-details">
            <a href="#" class="user-hover user-avatar" style="
                background-image:
                url('$github.getGravatarUrl()'),
                url(data:image/png;base64,)
">
                Some user</a>
            commited some code <a href="$entry.url"><abbr class="sha"> $entry.getShortSha()</abbr></a>
            in <a href="$entry.getRepoUrl()">$entry.repo</a>
            <abbr class="date" title="$entry.getDate()">$entry.getRelativeDate()</abbr>

        </div>
        <div class="action-body">
            $entry.getText()
        </div>
    </div>
    <div class="twixi-wrap concise actionContainer">
        <a class="twixi" href="#"><span class="icon twixi-opened"><span>Show</span></span></a>
        <div class="action-links">
            <a class="icon icon-github" href="$entry.url"><span>Github</span></a>
        </div>
        <div class="action-details">
            Commit <a href="$entry.url"><abbr class="sha"> $entry.getShortSha()</abbr></a>
            in <a href="$entry.getRepoUrl()">$entry.repo</a>
            <abbr class="date" title="$entry.getDate()">$entry.getRelativeDate()</abbr>
        </div>
    </div>
</div>
#end
</div>

